        <description/>
        <name>getCurrentFY</name>
        <script><![CDATA[function getCurrentFY(){
	
	
	// get Current fisical year
	var gDate = new GlideDateTime().getDisplayValue();
	var gDT = new GlideDateTime(gDate);
	var year=gDT.getYearUTC();
	var month=gDT.getMonthUTC();
	var fisicalYear;
	if (month>9){
		fisicalYear = year+1;
	}
	else
			fisicalYear=year;
		
	return fisicalYear.toString();    

}
]]></script>

        <description>Hide Submit button when user craete a new service.</description>
        <field>u_contact_email</field>
        <global>true</global>
        <isolate_script>false</isolate_script>
        <messages/>
        <name>Hide Submit button</name>
        <order/>
        <script><![CDATA[function onLoad() {
	//hide Submit button on load
	
	var refs = document.getElementsByTagName("BUTTON");

  //Remove the Submit button
  if (refs) {
    for (var i=0; i < refs.length; i++) {
      var ref = refs(i);
      var inner = ref.innerHTML;
      //check for the 'Submit' value on the button
      if (inner == ' Submit '){
        ref.style.display = '';
      }
    }
  }
	
}]]></script>

        <name>Validate Income Object Class</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
	if (isLoading) {
		return;
	}
	
	if (newValue == '') {
		g_form.hideFieldMsg('u_income_object_class');
		return;
		
	}
	
	var rx = new RegExp(/^\d{4}$/);
	
	if (!rx.test(newValue)) {
		g_form.hideFieldMsg('u_income_object_class');
		g_form.setValue('u_income_object_class', '');
		g_form.showFieldMsg('u_income_object_class', 'Please enter 4 digits number only.', 'error');
		
	} else {
		
		
		g_form.hideFieldMsg('u_income_object_class');
		
	}
}]]></script>
